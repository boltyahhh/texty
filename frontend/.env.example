# AWS Lambda Backend
NEXT_PUBLIC_AWS_API_GATEWAY_URL=https://your-api-gateway-url.amazonaws.com/prod
NEXT_PUBLIC_AWS_WEBSOCKET_URL=wss://your-websocket-url.amazonaws.com/prod
AWS_REGION=us-east-1
AWS_API_KEY=your-aws-api-key

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
NEXT_PUBLIC_OPENAI_MODEL=gpt-3.5-turbo

# Application Configuration
NEXT_PUBLIC_APP_URL=https://your-app.vercel.app
NEXT_PUBLIC_ENVIRONMENT=production

# Analytics
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=your-analytics-id

# Security
NEXTAUTH_SECRET=your-nextauth-secret
NEXTAUTH_URL=https://your-app.vercel.app